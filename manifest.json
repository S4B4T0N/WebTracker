{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "service_worker.js"
  },
  "host_permissions": [
    "https://*.supabase.co/*",
    "https://*.supabase.in/*"
  ],
  "permissions": [
    "debugger",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "settings.html",
    "default_icon": "hello_extensions.png"
  }
}
